    <script>
        window.onload = function exampleFunction() {

            if (sessionStorage.getItem("lastactive") != undefined) {
                var x = sessionStorage.getItem("lastactive");
                var xx = document.getElementById(x);
                xx.className += " bg-success";
            }
        }

        function act(id) {
            sessionStorage.setItem("lastactive", id);
        }

        function handleKeyPress(e) {
            let newValue = e.target.value + e.key;
            if (
                // It is not a number nor a control key?
                isNaN(newValue) &&
                e.which != 8 && // backspace
                e.which != 17 && // ctrl
                newValue[0] != '-' || // minus
                // It is not a negative value?
                newValue[0] == '-' &&
                isNaN(newValue.slice(1)))
                e.preventDefault(); // Then don't write it!
        }
    </script>
