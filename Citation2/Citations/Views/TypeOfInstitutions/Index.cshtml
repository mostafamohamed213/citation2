@model IEnumerable<Citations.Models.TypeOfInstitution>

@{
    ViewData["Title"] = "Index";
}

<h1>انواع المؤسسات</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TypeName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Active)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TypeName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Active)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.TypeInstitutionid">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.TypeInstitutionid">Details</a> @*|
                <a asp-action="Delete" asp-route-id="@item.TypeInstitutionid">Delete</a>*@
            </td>
        </tr>
}
    </tbody>
</table>
