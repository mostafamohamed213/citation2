@model Citations.Models.FacultyInstitution

@{
    ViewData["Title"] = "Edit";
}

 

<h4>FacultyInstitution</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="FacultyInstitutionid" />
            <div class="form-group">
                <label asp-for="Institutionid" class="control-label"></label>
                <select asp-for="Institutionid" class="form-control" asp-items="ViewBag.Institutionid"></select>
                <span asp-validation-for="Institutionid" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Facultyid" class="control-label"></label>
                <select asp-for="Facultyid" class="form-control" asp-items="ViewBag.Facultyid"></select>
                <span asp-validation-for="Facultyid" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
