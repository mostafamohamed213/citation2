@model IEnumerable<Citations.Models.PositionJob>

@{
    ViewData["Title"] = "Index";
}

 

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.PositionJob1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Active)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PositionJob1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Active)
            </td>
            
            <td>
                <a asp-action="Edit" asp-route-id="@item.PositionJobid" class="fa fa-edit" title="Edit"></a> |
                <a asp-action="Details" asp-route-id="@item.PositionJobid" class="fa fa-eye" title="Details"></a> |
                @*<a asp-action="Delete" asp-route-id="@item.PositionJobid" class="fa fa-trash" title="Delete"></a>*@
              
            </td>
       
           
        </tr>
}
    </tbody>
</table>
